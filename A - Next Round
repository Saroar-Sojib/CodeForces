
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,k;
    cin>>n>>k;
    int arr[n],count=0;
    for(int i=1;i<=n;i++)
    {
        cin>>arr[i];
 
    }
    for(int i=1;i<=n;i++)
    {
        if(arr[i]>0 && arr[i]>=arr[k])
            count++;
    }
    cout<<count<<endl;
 
    return 0;
}
